<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chetan.timeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".days_name" />
        <activity android:name=".Monday" />
        <activity android:name=".Tuesday" />
        <activity android:name=".Wednsday" />
        <activity android:name=".Thursday" />
        <activity android:name=".Friday" />
        <activity android:name=".save_data"
            android:label="Save Data">
        </activity>
        <activity android:name=".Saturday" />
        <activity android:name=".Sunday" />
        <activity android:name=".Developers" />
        <activity android:name=".card_view" />
        <provider
            android:authorities="com.example.chetan.time.TimeTable"
            android:name=".database"/>


        <receiver android:name=".MyBroadcastReciever" />
    </application>
    <uses-permission android:name="android.permission.VIBRATE"/>

</manifest>